% NOTE
\newcommand{\note}[1]{
\vshift\samepage{\noindent {\sl Note}\myline\vspace{-0.24in}}
 \offsetenumerate{#1} }

% SPECIAL NOTE
\newcommand{\specialnote}[2]{
\vshift\samepage{\noindent {\sl #1}\myline\vspace{-0.24in}}\\#2}

